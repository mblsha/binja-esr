{
  "include": [
    "sc62015/pysc62015"
  ],
  "exclude": [
    "sc62015/pysc62015/hw-test"
  ],
  "pythonVersion": "3.11",
  "typeCheckingMode": "basic",
  "stubPath": "./stubs",
  "reportMissingImports": "none",
